        },
        {
          "src": "/api/devices/([^/]+)/device-history",
          "dest": "api.js",
          "methods": ["GET"]
        },
        {
          "src": "/api/users/([^/]+)/devices",
          "dest": "api.js",
          "methods": ["GET"]
        },
        {
          "src": "/api/send-command",
          "dest": "api.js",
          "methods": ["POST"]
        },
        {
          "src": "/api/authenticate",
          "dest": "api.js",
          "methods": ["POST"]
        },
        {
          "src": "/api/register",
          "dest": "api.js",
          "methods": ["POST"]
        },
        {
          "src": "/api/devices",
          "dest": "api.js",
          "methods": ["POST", "GET"]
        },
        {
          "src": "/(.*)",
          "dest": "public/generated-docs/$1"
        }
